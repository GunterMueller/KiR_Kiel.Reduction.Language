#! /bin/awk -f

BEGIN	{
		printf ("/*  measure names\n\n");
                printf ("    This file is automatically generated\n");
                printf ("    DO NOT EDIT BELOW ! */\n\n");
		printf ("char *mess_names[] = {\n");  

                mindex = 1;
	}

/^\t/	{
		if(NF) {
                        if (mindex == 1) {
                          printf("  \"\"\n, \"%s\"\n", $1); }
                        else {
                          printf(", \"%s\"\n", $1); }
                        mindex++;
		}
	}
END {
		printf ("};\n");
                printf ("\n#define D_MAX_NAMES %d\n", mindex-1);
}
